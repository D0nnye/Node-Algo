{{>partials/header}}
<a href="/cadastro">Adicionar Usuário</a>

<table border="1" width="100%">

    <tr>
        <th>ID</th>
        <th>NOME</th>
        <th>EMAIL</th>
        <th>AÇÕES</th>
    </tr>

    {{#users}}
    <tr>
        <td>{{id}}</td>
        <td>{{nome}}</td>
        <td>{{email}}</td>
        <td>
            <a href="/editar/{{id}}"><img src="/public/css/bin.png" alt="">[Editar]</a>
            <a href="/excluir/{{id}}" onclick="return confirm('Deseja excluir?')"><img src="/public/css/escrevendo.png">[Exluir]</a>
        </td>
    </tr>
    {{/users}}

</table>

{{>partials/footer}}